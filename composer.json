{
  "repositories": [
    {
      "type":"composer",
      "url":"http://wpackagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "wordpressNL",
        "type": "webroot",
        "version": "3.9.2",
        "dist": {
          "type": "zip",
          "url": "https://downloads.wordpress.org/release/nl_NL/wordpress-3.9.2.zip"
        },
        "require" : {
          "fancyguy/webroot-installer": "1.1.0"
        }
      }
    },
    {
      "type": "vcs",
      "url": "ssh://filevault.localdomain/home/git/private/akvs-plugin.git"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "wordpressNL": "3.9.2",
    "fancyguy/webroot-installer": "1.1.0",
    "vlucas/phpdotenv": "1.0.6",
    "wpackagist-plugin/developer": "1.2.5",
    "wpackagist-plugin/regenerate-thumbnails": "2.2.4",
    "wpackagist-plugin/wordpress-importer": "0.6.1",
    "wpackagist-plugin/wp-pagenavi": "2.85",
    "kdb/akvs-plugin": "dev-master"
  },
  "extra": {
    "installer-paths": {
      "app/plugins/{$name}/": ["type:wordpress-plugin"],
      "app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "webroot-dir": "wp",
    "webroot-package": "wordpressNL"
  }
}
