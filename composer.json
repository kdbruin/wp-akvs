{
  "repositories": [
    {
      "type":"composer",
      "url":"http://wpackagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "wordpressNL",
        "type": "webroot",
        "version": "3.9.1",
        "dist": {
          "type": "zip",
          "url": "https://downloads.wordpress.org/release/nl_NL/wordpress-3.9.1.zip"
        },
        "require" : {
          "fancyguy/webroot-installer": "1.1.0"
        }
      }
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "wordpressNL": "3.9.1",
    "fancyguy/webroot-installer": "1.1.0",
    "vlucas/phpdotenv": "1.0.6",
    "wpackagist-plugin/regenerate-thumbnails": "2.2.4",
    "wpackagist-plugin/smpl-shortcodes": "1.0.16",
    "wpackagist-plugin/wp-pagenavi": "2.85",
    "wpackagist-theme/smpl-skeleton": "*"
  },
  "extra": {
    "installer-paths": {
      "app/plugins/{$name}/": ["type:wordpress-plugin"],
      "app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "webroot-dir": "wp",
    "webroot-package": "wordpressNL"
  }
}
