{
  "repositories": [
    {
      "type":"composer",
      "url":"http://wpackagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "wordpressNL",
        "type": "webroot",
        "version": "4.1.1",
        "dist": {
          "type": "zip",
          "url": "https://downloads.wordpress.org/release/nl_NL/wordpress-4.1.1.zip"
        },
        "require" : {
          "fancyguy/webroot-installer": "1.1.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
	"name": "simone",
        "type": "wordpress-theme",
	"version": "2.0.3",
        "source": {
	  "type": "git",
	  "url": "https://github.com/mor10/simone.git",
	  "reference": "master"
        }
      }
    },
    {
      "type": "vcs",
      "url": "ssh://filevault.localdomain/home/git/private/akvs-plugin.git"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "wordpressNL": "4.1.1",
    "fancyguy/webroot-installer": "1.1.0",
    "vlucas/phpdotenv": "1.0.6",
    "wpackagist-plugin/developer": "1.2.5",
    "wpackagist-plugin/easy-table": "1.5.2",
    "wpackagist-plugin/regenerate-thumbnails": "2.2.4",
    "wpackagist-plugin/tabby-responsive-tabs": "1.2.1",
    "wpackagist-plugin/wordpress-importer": "0.6.1",
    "kdb/akvs-plugin": "dev-master",
    "simone": "2.0.3"
  },
  "extra": {
    "installer-paths": {
      "app/plugins/{$name}/": ["type:wordpress-plugin"],
      "app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "webroot-dir": "wp",
    "webroot-package": "wordpressNL"
  }
}
